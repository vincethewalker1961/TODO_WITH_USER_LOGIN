<div class="container">
  <div class="row">
    <h1 class="text-center">User: <%= current_user.name %></h1>
    <div class="col-md-8 col-md-offset-4">
        <p>
            <%= link_to "Create a To Do", new_todo_path, class: "btn btn-success" %>
            <%= link_to "log Out", login_path, method: :delete, class: "btn btn-danger" %>
            <%= link_to "Todo List", todos_path, class: "btn btn-primary" %>
            <%= link_to "Edit Me", user_path(current_user), class: "btn btn-warning" %>
            
        </p>
          <table class="row center">
            <tr>
              <th>Name</th>
              <th class="spacer">gkdgdkdgklbkLJDGLK</th>
              <th>Description</th>
            </tr>
            <% @users.each do |user| %>
              <tr class ="well profile-black-white-thin row <%= cycle('profile-black-white-thin-b', '') %>"> 
                <td class="texties"><%= user.name %></td>
                <td class="texties"><%= user.email %></td>
            </tr>
          <% end %>
        </table>
    </div>
  </div>
</div>